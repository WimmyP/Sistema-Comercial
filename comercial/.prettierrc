{
    "plugins": ["prettier-plugin-svelte"],
    "printWidth": 180,
    "singleQuote": false,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "quoteProps": "as-needed",
    "jsxBracketSameLine": false,
    "arrowParens": "always"
}
  